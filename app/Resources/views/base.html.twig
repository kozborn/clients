<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
          <link href="{{ asset('bundles/client/css/styles.css') }}" type="text/css" rel="stylesheet" />
          <link href="{{ asset('bundles/client/bootstrap-3.2.0/css/bootstrap.css') }}" type="text/css" rel="stylesheet" />
          <link href="{{ asset('bundles/client/js/jquery-ui-1.11.1.custom/jquery-ui.css') }}" type="text/css" rel="stylesheet" />
          {# <link href="{{ asset('bundles/client/js/Flexigrid-master/css/flexigrid.css') }}" type="text/css" rel="stylesheet" /> #}
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        <div class="container-fluid">
            {% block body %}{% endblock %}
        </div>
        {% block javascripts %}
            <script type="text/javascript" src="{{ asset('bundles/client/js/jquery.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bundles/client/js/script.js') }}"></script>
            <script type="text/javascript" src="{{ asset('bundles/client/js/jquery-ui-1.11.1.custom/jquery-ui.js') }}"></script>
        {% endblock %}
    </body>
</html>
